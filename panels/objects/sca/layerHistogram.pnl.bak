dyn_string listOfItemsToHist;
int xRef=$xRef;
int yRef=$yRef;


void updateHisto()
{
 
 for(int i=1;i<=dynlen(listOfItemsToHist);i++)
  {   
   removeSymbol(myModuleName(), myPanelName(), \"HistoPart_\"+listOfItemsToHist[i]+\"_\"+\"VMM\");  
   removeSymbol(myModuleName(), myPanelName(), \"HistoPart_\"+listOfItemsToHist[i]+\"_\"+\"SCA\");  
   removeSymbol(myModuleName(), myPanelName(), \"HistoPart_\"+listOfItemsToHist[i]+\"_\"+\"FEAST_MMFE8\");  
   removeSymbol(myModuleName(), myPanelName(), \"HistoPart_\"+listOfItemsToHist[i]+\"_\"+\"FEAST_ADDC\"); 
   removeSymbol(myModuleName(), myPanelName(), \"HistoPart_\"+listOfItemsToHist[i]+\"_\"+\"FEAST_L1DDC\");  
   removeSymbol(myModuleName(), myPanelName(), \"HistoPart_\"+listOfItemsToHist[i]+\"_\"+\"GBTX_ADDC\");  
   removeSymbol(myModuleName(), myPanelName(), \"HistoPart_\"+listOfItemsToHist[i]+\"_\"+\"GBTX_L1DDC\");  
  } 
  
  int layer=layerCombo.selectedPos();
  
  string side=$side;  
  
  dyn_string totalListOfBoards;
  totalListOfBoards=dpNames(\"*\",\"fwScaSCA\");
  

  if(boardTypeCombo.number()==0)
  {      

  dynClear(listOfItemsToHist);
  
  for(int i=1;i<=dynlen(totalListOfBoards);i++)
  {
  strreplace(totalListOfBoards[i],getSystemName(),\"\");
  
  if(patternMatch(\"MMFE8\"+\"_L\"+layer+\"P*_\"+side+\"*\",totalListOfBoards[i]))  
   {
    dynAppend(listOfItemsToHist,totalListOfBoards[i]);
   }
 
  }
  
  int x=1720;
  int y=yRef+50;
  for(int i=dynlen(listOfItemsToHist);i>0;i--)
  {
   addSymbol(myModuleName(),myPanelName(),\"objects/sca/layerHistogramPart.pnl\",\"HistoPart_\"+listOfItemsToHist[i]+\"_\"+\"VMM\",makeDynString(\"$board:\"+listOfItemsToHist[i],\"$mode:\"+\"VMM\"),x,y,0,1,1);
   x=x-110;
   hist1.text(\"VMM\");
  }
  
  x=1720;
  y=y+80;
  for(int i=dynlen(listOfItemsToHist);i>0;i--)
  {
   addSymbol(myModuleName(),myPanelName(),\"objects/sca/layerHistogramPart.pnl\",\"HistoPart_\"+listOfItemsToHist[i]+\"_\"+\"SCA\",makeDynString(\"$board:\"+listOfItemsToHist[i],\"$mode:\"+\"SCA\"),x,y,0,1,1);
   x=x-110;
   hist2.text(\"SCA\");
  }
  
  x=1720;
  y=y+80;
  for(int i=dynlen(listOfItemsToHist);i>0;i--)
  {
   addSymbol(myModuleName(),myPanelName(),\"objects/sca/layerHistogramPart.pnl\",\"HistoPart_\"+listOfItemsToHist[i]+\"_\"+\"FEAST_MMFE8\",makeDynString(\"$board:\"+listOfItemsToHist[i],\"$mode:\"+\"FEAST_MMFE8\"),x,y,0,1,1);
   x=x-110;
   hist3.text(\"FEAST\");
  }
    
  }
  
  
 
  // ************************************************************************************
  
  if(boardTypeCombo.number()==1)
  {
  //L1DDC
    dyn_string l1ddcListOfBoards;  

  
  dynClear(listOfItemsToHist);  
  for(int i=1;i<=dynlen(totalListOfBoards);i++)
  {
  strreplace(totalListOfBoards[i],getSystemName(),\"\");
  
  if(patternMatch(\"L1DDC\"+\"_L\"+layer+\"P*_\"+side+\"*\",totalListOfBoards[i]))  
   {
    dynAppend(listOfItemsToHist,totalListOfBoards[i]);
    dynAppend(l1ddcListOfBoards,totalListOfBoards[i]);
   }
     
  }
  
  int x=1200;
  int y=yRef+50;
  for(int i=dynlen(l1ddcListOfBoards);i>0;i--)
  {
   addSymbol(myModuleName(),myPanelName(),\"objects/sca/layerHistogramPart.pnl\",\"HistoPart_\"+l1ddcListOfBoards[i]+\"_\"+\"GBTX_L1DDC\",makeDynString(\"$board:\"+l1ddcListOfBoards[i],\"$mode:\"+\"GBTX_L1DDC\"),x,y,0,1,1);
   x=x-110;
   hist1.text(\"GBTX\");
  }
  
  x=1200;
  y=y+80;
  for(int i=dynlen(l1ddcListOfBoards);i>0;i--)
  {
   addSymbol(myModuleName(),myPanelName(),\"objects/sca/layerHistogramPart.pnl\",\"HistoPart_\"+l1ddcListOfBoards[i]+\"_\"+\"SCA\",makeDynString(\"$board:\"+l1ddcListOfBoards[i],\"$mode:\"+\"SCA\"),x,y,0,1,1);
