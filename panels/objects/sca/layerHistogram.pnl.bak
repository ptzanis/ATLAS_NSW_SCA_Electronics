  for(int i=dynlen(l1ddcListOfBoards);i>0;i--)
  {
   addSymbol(myModuleName(),myPanelName(),\"objects/sca/layerHistogramPart.pnl\",\"HistoPart_\"+listOfItemsToHist[i]+\"_\"+\"FEAST_L1DDC\",makeDynString(\"$board:\"+l1ddcListOfBoards[i],\"$mode:\"+\"FEAST_L1DDC\"),x,y,0,1,1);
   x=x-110;
   hist3.text(\"FEAST\");
  }
  
  //ADDC
    dyn_string addcListOfBoards;  

  for(int i=1;i<=dynlen(totalListOfBoards);i++)
  {
  strreplace(totalListOfBoards[i],getSystemName(),\"\");
  
  if(patternMatch(\"ADDC\"+\"_L\"+layer+\"P*_\"+side+\"*\",totalListOfBoards[i]))  
   {
    dynAppend(listOfItemsToHist,totalListOfBoards[i]);
    dynAppend(addcListOfBoards,totalListOfBoards[i]);
   }
     
  }

  int x=760;
